{
  "name": "bvm",
  "version": "1.0.1",
  "description": "The native version manager for Bun. Cross-platform, shell-agnostic, and zero-dependency.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "npx bun run src/index.ts",
    "test": "npx bun test test/*.ts",
    "bvm": "npx bun run src/index.ts",
    "bvm:sandbox": "mkdir -p \"$PWD/.sandbox-home\" && HOME=\"$PWD/.sandbox-home\" npx bun run src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bvm-cli/bvm.git"
  },
  "keywords": [
    "bun",
    "version-manager",
    "cli",
    "bvm"
  ],
  "author": "bvm-cli",
  "license": "MIT",
  "type": "commonjs",
  "dependencies": {
    "@types/cli-progress": "^3.11.6",
    "@types/inquirer": "^9.0.9",
    "@types/node": "^24.10.2",
    "@types/semver": "^7.7.1",
    "@types/unzipper": "^0.10.11",
    "axios": "^1.13.2",
    "cac": "^6.7.14",
    "chalk": "^5.6.2",
    "cli-progress": "^3.12.0",
    "commander": "^14.0.2",
    "extract-zip": "^2.0.1",
    "inquirer": "^13.0.2",
    "ora": "^9.0.0",
    "semver": "^7.7.3",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "unzipper": "^0.12.3"
  },
  "private": true,
  "devDependencies": {
    "@types/bun": "latest",
    "bun": "^1.3.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
